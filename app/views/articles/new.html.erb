<h1>Nuevo Art√≠culo</h1>

<%= form_for @article do |f| %>
  <div>
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :body %> <br>
    <%= f.text_area :body, rows: 5 %>
  </div>
  <div>
    <%= f.check_box :published %> <%= f.label :published %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
